{
	"name": "aname-backend",
	"version": "0.1.0",
	"description": "aName backend",
	"main": "src/index.js",
	"scripts": {
		"prod": "DEBUG=aname-backend:* node --inspect=0.0.0.0 src/index.js",
		"dev": "cross-env NODE_ENV=development DEBUG=aname-backend:* node --watch --no-warnings --trace-warnings --inspect=0.0.0.0 src/index.js",
		"docker:dev": "cross-env node generateCert.js && docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --remove-orphans",
		"test": "cross-env REDIS_URL=redis://localhost:16379 node --experimental-vm-modules ./node_modules/.bin/jest",
		"test:watch": "cross-env REDIS_URL=redis://localhost:16379 node --experimental-vm-modules ./node_modules/.bin/jest --verbose --watch",
		"generate-cert": "node generateCert.js"
	},
	"author": "june07",
	"license": "MIT",
	"dependencies": {
		"@keyv/valkey": "^1.0.3",
		"@noble/curves": "^1.8.1",
		"@octokit/auth-oauth-user": "^5.1.3",
		"@octokit/core": "^6.1.4",
		"@octokit/plugin-retry": "^7.2.0",
		"@octokit/rest": "^21.1.1",
		"@socket.io/redis-streams-adapter": "^0.2.2",
		"async": "^3.2.6",
		"bullmq": "^5.45.1",
		"cache-manager": "^6.4.1",
		"compression": "^1.8.0",
		"connect-mongo": "^5.1.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dd-trace": "^5.45.0",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"express-jwt": "^8.5.1",
		"express-jwt-authz": "^2.4.1",
		"express-mongo-sanitize": "^2.2.0",
		"express-session": "^1.18.1",
		"got": "^14.4.6",
		"helmet": "^8.1.0",
		"http-status": "^2.1.0",
		"joi": "^17.13.3",
